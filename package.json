{
  "name": "ngx-laydate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "clean": "rm -rf dist",
    "build:lib": "ng build ngx-laydate",
    "build:lib:prod": "ng build ngx-laydate --prod",
    "update:readme": "cpr README.md dist/ngx-laydate/README.md -o",
    "build:dist": "run-s clean build:lib:prod update:readme",
    "clean:docs": "rm -rf docs",
    "build:docs": "ng build example --prod --output-path docs --base-href /ngx-laydate/",
    "build:compodoc": "compodoc -p projects/ngx-laydate/tsconfig.lib.json -d docs/api-docs --theme stripe",
    "build:ghpages": "npx angular-cli-ghpages --dir dist/example",
    "build:deploy": "run-s clean build:docs build:compodoc build:ghpages",
    "build:ci": "run-s clean:docs build:docs build:compodoc",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@compodoc/compodoc": "^1.1.19",
    "layui-laydate": "^5.3.1",
    "marked": "^4.2.12",
    "ng-zorro-antd": "^10.2.2",
    "ngx-markdown": "^10.1.1",
    "prismjs": "^1.29.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~10.2.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^4.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.1",
    "html-loader": "^1.3.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
